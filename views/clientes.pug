extends layout

block content
  h1 Clientes
  table.table.table-bordered
    thead
      tr
        th Nome
        th Sobrenome
        th Email
        th Idade
    tbody
      each cliente in clientes
        tr
          td= cliente.nome
          td= cliente.sobrenome
          td= cliente.email
          td= cliente.idade
